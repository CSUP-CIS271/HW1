package hw1;

import java.util.Scanner;

/**
 *
 * @author Vitor Winckler
 */
public class HW1 {

    private String f_name;
    private String l_name;
    private String m_name;

    public HW1() {
    }

    public HW1(String f_name, String l_name, String m_name) {
        this.f_name = f_name;
        this.l_name = l_name;
        this.m_name = m_name;
    }

    public String getF_name() {
        return f_name;
    }

    public void setF_name(String f_name) {
        this.f_name = f_name;
    }

    public String getL_name() {
        return l_name;
    }

    public void setL_name(String l_name) {
        this.l_name = l_name;
    }

    public String getM_name() {
        return m_name;
    }

    public void setM_name(String m_name) {
        this.m_name = m_name;
    }

    @Override
    public String toString() {
        return f_name + " "+ m_name  + " " + l_name;
    }

    public String toStringReverse(){
        return l_name+ " " + m_name + " " + f_name;
    }
           
    
    public static void main(String[] args) {
        HW1 person = new HW1();
        System.out.println("Insert Your Name:");
        Scanner sc = new Scanner(System.in);
        person.setF_name(sc.next());
        System.out.println("Insert Your Last Name:");
        person.setL_name(sc.next());
        System.out.println("Insert Your Middle Name:");
        person.setM_name(sc.next());
        System.out.println("Normal :" +person.toString());
        System.out.println("Reverse :" +person.toStringReverse());
        
    }
    
}
